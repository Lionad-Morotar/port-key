{
  "name": "@lionad/port-key",
  "version": "0.4.0",
  "description": "A simple, practical port naming strategy",
  "type": "module",
  "types": "./src/port-key.d.ts",
  "exports": {
    ".": {
      "types": "./src/port-key.d.ts",
      "default": "./src/port-key.js"
    }
  },
  "keywords": [
    "portkey",
    "port-key",
    "port",
    "port number",
    "naming",
    "strategy",
    "method",
    "mcp"
